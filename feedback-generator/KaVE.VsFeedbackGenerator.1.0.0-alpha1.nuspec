<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
    <metadata>
        <id>KaVE.VsFeedbackGenerator</id>
        <version>1.0.0-alpha1</version>
        <title>KaVE Projekt</title>
        <authors>Sebastian Proksch,Sven Amann,Uli Fahrer,Dennis Albrecht</authors>
        <owners>Technische Universitaet Darmstadt</owners>
        <licenseUrl>http://www.apache.org/licenses/LICENSE-2.0.txt</licenseUrl>
        <projectUrl>http://kave.cc/</projectUrl>
        <iconUrl>http://kave.cc/icon.png</iconUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Observes how you use Visual Studio and creates usage feedback.</description>
        <releaseNotes></releaseNotes>
        <copyright>Copyright 2013, 2014</copyright>
        <dependencies>
            <dependency id="ReSharper" version="[8.2, 8.3)" />
        </dependencies>
