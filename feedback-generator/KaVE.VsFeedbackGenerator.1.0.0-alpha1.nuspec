    <files>
		<!-- WARNING: We cannot have shared DLLs in vAny. All required DLLs need to be in v8.x in order for the plugin to load correctly. -->
		
		<!-- Plugin for ReSharper 8.2 -->
			<!-- KaVE Assemblies -->
				<file src="KaVE.VsFeedbackGenerator\bin\Debug\KaVE.Commons.dll" target="ReSharper\v8.2\plugins\KaVE.Commons.dll" />
				<file src="KaVE.VsFeedbackGenerator\bin\Debug\KaVE.ReSharper.Commons.dll" target="ReSharper\v8.2\plugins\KaVE.ReSharper.Commons.dll" />
				<file src="KaVE.VsFeedbackGenerator\bin\Debug\KaVE.VsFeedbackGenerator.dll" target="ReSharper\v8.2\plugins\KaVE.VsFeedbackGenerator.dll" />
			<!-- 3rd-Party Dependencies -->
				<file src="KaVE.VsFeedbackGenerator\bin\Debug\Newtonsoft.Json.dll" target="ReSharper\v8.2\plugins\Newtonsoft.Json.dll" />
				<file src="KaVE.VsFeedbackGenerator\bin\Debug\Hardcodet.Wpf.TaskbarNotification.dll" target="ReSharper\v8.2\plugins\Hardcodet.Wpf.TaskbarNotification.dll" />
        <file src="3rd-party\MsgBox.dll" target="ReSharper\v8.2\plugins\MsgBox.dll" />
        <file src="3rd-party\UserNotification.dll" target="ReSharper\v8.2\plugins\UserNotification.dll" />
        <file src="3rd-party\smileNet.dll" target="ReSharper\v8.2\plugins\smileNet.dll" />
        <file src="3rd-party\System.Net.Http.dll" target="ReSharper\v8.2\plugins\System.Net.Http.dll" />
    </files>
</package>