<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
    <metadata>
        <id>KaVE.VsFeedbackGenerator</id>
        <version>1.0.0-alpha1</version>
        <title>KaVE Projekt (DATEV)</title>
